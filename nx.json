{
  "$schema": "./node_modules/nx/schemas/nx-schema.json",
  "targetDefaults": {
    "build": {
      "outputs": [
        "{projectRoot}/dist"
      ],
      "cache": true
    },
    "start": {
      "cache": true
    },
    "start:dev": {
      "cache": true
    },
    "start:debug": {
      "cache": true
    },
    "start:prod": {
      "cache": true
    },
    "lint": {
      "cache": true
    },
    "lint:fix": {
      "cache": true
    },
    "format": {
      "cache": true
    },
    "format:check": {
      "cache": true
    },
    "test": {
      "cache": true
    },
    "test:watch": {
      "cache": true
    },
    "test:cov": {
      "cache": true
    },
    "test:debug": {
      "cache": true
    },
    "test:e2e": {
      "cache": true
    }
  },
  "plugins": [
    {
      "plugin": "@nx/eslint/plugin",
      "options": {
        "targetName": "eslint:lint"
      }
    },
    {
      "plugin": "@nx/jest/plugin",
      "options": {
        "targetName": "jest:test"
      }
    }
  ],
  "nxCloudId": "6894fc21af67c125c4e9fca0"
}
