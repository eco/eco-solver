{
  "name": "eco-solver",
  "version": "1.5",
  "description": "An implementation filler/solver for the Eco Routes protocol",
  "homepage": "https://github.com/eco/eco-solver#readme",
  "bugs": {
    "url": "https://github.com/eco/eco-solver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eco/eco-solver.git"
  },
  "author": "Eco Engineering <eng@eco.com>",
  "license": "MIT",
  "keywords": [
    "solver",
    "filler",
    "rollup",
    "eco routes",
    "routes",
    "bend",
    "protocol"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nx build eco-solver",
    "build:nx": "nx build eco-solver",
    "build:all": "nx run-many -t build",
    "build:affected": "nx affected -t build",
    "start": "NODE_OPTIONS='--max-old-space-size=4096' nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:nx": "nx serve eco-solver",
    "start:prod": "node dist/main",
    "cli": "ts-node src/commander/command-main.ts",
    "lint": "nx run-many -t lint",
    "lint:fix": "nx run-many -t lint --fix",
    "lint:affected": "nx affected -t lint",
    "prettier": "nx format:check",
    "prettier:fix": "nx format:write",
    "format": "nx lint:fix && pnpm prettier:fix",
    "format:check": "nx format:check",
    "test": "nx run-many -t test",
    "test:affected": "nx affected -t test",
    "test:affected-only": "nx affected:test",
    "test:watch": "nx test eco-solver --watch",
    "test:cov": "nx run-many -t test --coverage",
    "test:debug": "nx test eco-solver --inspect-brk",
    "test:e2e": "nx run-many -t e2e",
    "test:e2e:affected": "nx affected -t e2e",
    "graph": "nx graph",
    "dep-graph": "nx dep-graph",
    "affected:graph": "nx affected:graph",
    "reset": "nx reset",
    "nx:connect": "nx connect-to-nx-cloud",
    "circular:check": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --no-color .",
    "circular:check:ci": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --no-color . || true",
    "circular:check:strict": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --no-color --no-spinner .",
    "circular:check:apps": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx|libs' --no-color apps/",
    "circular:check:libs": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx|apps' --no-color libs/",
    "circular:check:with-graph": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --image circular-deps.svg .",
    "circular:check:json": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --json . > circular-deps.json",
    "circular:check:detailed": "madge --circular --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --warning --no-spinner --no-color .",
    "circular:analyze": "madge --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --dot . > dependency-tree.dot",
    "circular:orphans": "madge --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --orphans --no-color .",
    "circular:leaves": "madge --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --leaves --no-color .",
    "circular:summary": "madge --extensions ts,js --exclude 'node_modules|dist|coverage|tmp|.nx' --summary --no-color .",
    "circular:test-eslint": "./scripts/test-eslint-rules.sh"
  },
  "dependencies": {
    "@0xsquid/sdk": "^2.10.0",
    "@0xsquid/squid-types": "^0.1.163",
    "@aws-sdk/client-kms": "^3.744.0",
    "@aws-sdk/client-secrets-manager": "^3.592.0",
    "@eco-foundation/chains": "1.0.45",
    "@eco-foundation/eco-kms-core": "^2.0.0",
    "@eco-foundation/eco-kms-provider-aws": "^2.0.0",
    "@eco-foundation/eco-kms-wallets": "^2.0.0",
    "@eco-foundation/routes-ts": "^2.8.14",
    "@launchdarkly/node-server-sdk": "^9.7.1",
    "@liaoliaots/nestjs-redis-health": "^9.0.4",
    "@lifi/sdk": "^3.6.6",
    "@lit-protocol/auth-helpers": "^7.0.4",
    "@lit-protocol/constants": "^7.0.4",
    "@lit-protocol/lit-node-client": "^7.0.4",
    "@lit-protocol/types": "^7.2.0",
    "@nestjs/bullmq": "^10.1.1",
    "@nestjs/cache-manager": "^3.0.0",
    "@nestjs/common": "^10.4.16",
    "@nestjs/config": "^3.2.2",
    "@nestjs/core": "^10.4.16",
    "@nestjs/event-emitter": "^2.0.4",
    "@nestjs/mongoose": "^11.0.1",
    "@nestjs/platform-express": "^11.0.11",
    "@nestjs/swagger": "^11.0.3",
    "@nestjs/terminus": "^10.2.3",
    "@reservoir0x/relay-sdk": "^2.0.1",
    "@rhinestone/module-sdk": "^0.2.7",
    "@shelf/jest-mongodb": "^4.1.7",
    "@zerodev/ecdsa-validator": "^5.4.9",
    "@zerodev/sdk": "^5.4.40",
    "abitype": "^1.0.8",
    "asn1js": "^3.0.5",
    "axios": "0.21.4",
    "bullmq": "^5.8.5",
    "cache-manager": "^6.4.0",
    "canonicalize": "^2.1.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "config": "^3.3.11",
    "date-fns": "^3.6.0",
    "dayjs": "^1.11.13",
    "enumify": "^2.0.0",
    "ioredis": "^5.4.1",
    "lodash": "^4.17.21",
    "mongoose": "^8.9.5",
    "nest-commander": "^3.17.0",
    "nestjs-pino": "^4.1.0",
    "ox": "0.8.0",
    "permissionless": "^0.2.49",
    "pino-http": "^10.2.0",
    "posthog-node": "^5.1.1",
    "redlock": "^5.0.0-beta.2",
    "reflect-metadata": "^0.2.0",
    "rxjs": "^7.8.1",
    "table": "^6.8.2",
    "tslib": "^2.8.1",
    "typescript": "^5.8.3",
    "uuid": "^11.0.5",
    "viem": "^2.31.7"
  },
  "pnpm": {
    "overrides": {
      "ox": "0.8.0"
    }
  },
  "devDependencies": {
    "@golevelup/ts-jest": "^0.5.0",
    "@jest/globals": "^30.0.5",
    "@nestjs/axios": "^3.0.0",
    "@nestjs/cli": "^10.0.0",
    "@nestjs/schematics": "^10.0.0",
    "@nestjs/testing": "^10.4.16",
    "@nx/eslint-plugin": "^21.3.11",
    "@nx/jest": "^21.3.11",
    "@nx/js": "^21.3.11",
    "@nx/nest": "^21.3.11",
    "@nx/node": "^21.3.11",
    "@nx/webpack": "^21.3.11",
    "@nx/workspace": "21.3.11",
    "@types/config": "^3.3.5",
    "@types/express": "^4.17.17",
    "@types/jest": "^29.5.2",
    "@types/lodash": "^4.17.5",
    "@types/node": "^20.3.1",
    "@types/supertest": "^6.0.0",
    "@typescript-eslint/eslint-plugin": "^6.0.0",
    "@typescript-eslint/parser": "^6.0.0",
    "env-cmd": "^10.1.0",
    "eslint": "^8.42.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-import-resolver-typescript": "^4.4.4",
    "eslint-plugin-import": "^2.32.0",
    "eslint-plugin-prettier": "^5.0.0",
    "jest": "^29.5.0",
    "jest-config": "^29.7.0",
    "madge": "^8.0.0",
    "nx": "21.3.11",
    "prettier": "^3.0.0",
    "source-map-support": "^0.5.21",
    "supertest": "^6.3.3",
    "testcontainers": "^11.5.0",
    "ts-jest": "^29.1.0",
    "ts-loader": "^9.4.3",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "webpack": "^5.101.1",
    "webpack-cli": "^6.0.1"
  },
  "workspaces": [
    "libs/*/*"
  ]
}
