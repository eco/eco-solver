{
  "name": "eco-solver",
  "version": "1.5",
  "description": "An implementation filler/solver for the Eco Routes protocol",
  "homepage": "https://github.com/eco/eco-solver#readme",
  "bugs": {
    "url": "https://github.com/eco/eco-solver/issues"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/eco/eco-solver.git"
  },
  "author": "Eco Engineering <eng@eco.com>",
  "license": "MIT",
  "keywords": [
    "solver",
    "filler",
    "rollup",
    "eco routes",
    "routes",
    "bend",
    "protocol"
  ],
  "scripts": {
    "preinstall": "npx only-allow pnpm",
    "build": "nest build",
    "start": "NODE_OPTIONS='--max-old-space-size=4096' nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "node dist/main",
    "cli": "ts-node src/commander/command-main.ts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prettier": "prettier . --check",
    "prettier:fix": "prettier . --write",
    "format": "pnpm lint:fix && pnpm prettier:fix",
    "format:check": "pnpm lint && pnpm prettier",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "validate-datadog-schema": "claude .claude/commands/validate-datadog-schema.md"
  },
  "dependencies": {
