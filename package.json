    "@nestjs/schematics": "^11.0.0",
    "@nestjs/testing": "^11.0.0",
    "@nx/esbuild": "21.3.11",
    "@nx/eslint": "21.3.11",
    "@nx/eslint-plugin": "21.3.11",
    "@nx/jest": "21.3.11",
    "@nx/js": "^21.3.11",
    "@nx/nest": "^21.3.11",
    "@nx/node": "^21.3.11",
    "@nx/web": "21.3.11",
