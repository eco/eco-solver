{
  "version": "0.2.0",
  "configurations": [
    {
      "type": "node",
      "request": "launch",
      "name": "Debug eco-solver with Nx",
      "program": "${workspaceFolder}/dist/apps/eco-solver/main.js",
      "args": [],
      "cwd": "${workspaceFolder}",
      "env": {
        "NODE_ENV": "development",
        "NODE_CONFIG_DIR": "${workspaceFolder}/dist/apps/eco-solver/config"
      },
      "console": "integratedTerminal",
      "internalConsoleOptions": "neverOpen",
      "skipFiles": ["<node_internals>/**"],
      "sourceMaps": true,
      "outFiles": ["${workspaceFolder}/dist/apps/eco-solver/**/*.js"],
      "preLaunchTask": "nx:build:eco-solver:development",
      "stopOnEntry": false
    }
  ]
}
